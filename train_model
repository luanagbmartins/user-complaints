python -m luigi --module src.models.train_model TrainModel --FeatureExtraction-ngram-range '(1,2)' --FeatureExtraction-min-df 5 --FeatureExtraction-use-idf --FeatureExtraction-smooth-idf --FeatureExtraction-sublinear-tf --TrainModel-test-size 0.2 --TrainModel-shuffle --TrainModel-selected-model random_forest --TrainModel-n-estimators 100 --TrainModel-verbose 3 --local-scheduler